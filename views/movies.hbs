<div class="movies-content">
    {{#each movies}}
      <div class="card" style="width: 18rem;">
          <img class="card-img-top" src="{{this.image}}" alt="Card image" />
          <div class="card-body">
              <h5 class="card-title">{{this.title}}</h5>
              
              <a href="/movies/{this.id}}" class="btn btn-primary">See more</a>
          </div>
      </div>
    {{/each}}
</div>